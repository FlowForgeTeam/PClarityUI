<div class="sidebar-item {% if item.isActive %}active{% endif %}"
    data-page="{{ item.pageName }}"
    >
    <span class="nav-icon">
        {% include "../../assets/icons/" ~ item.icon ~ "-icon.svg" %}
    </span>

    {% if sidebarExpanded %}
        <span class="nav-label">{{ item.label }}</span>
    {% endif %}
</div>